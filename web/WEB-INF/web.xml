<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>welcome</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>viewCollegeServlet</servlet-name>
        <servlet-class>com.endicott.edu.ui.ViewCollegeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewCollegeServlet</servlet-name>
        <url-pattern>/viewCollege</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>welcomeServlet</servlet-name>
        <servlet-class>com.endicott.edu.ui.WelcomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>welcomeServlet</servlet-name>
        <url-pattern>/welcome</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>viewBuildingsServlet</servlet-name>
        <servlet-class>com.endicott.edu.ui.ViewBuildingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewBuildingsServlet</servlet-name>
        <url-pattern>/viewBuilding</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>viewStudentServlet</servlet-name>
        <servlet-class>com.endicott.edu.ui.ViewStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewStudentServlet</servlet-name>
        <url-pattern>/viewStudent</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>viewSportsServlet</servlet-name>
        <servlet-class>com.endicott.edu.ui.ViewSportsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewSportsServlet</servlet-name>
        <url-pattern>/viewSports</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>viewFacultyServlet</servlet-name>
        <servlet-class>com.endicott.edu.ui.ViewFacultyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewFacultyServlet</servlet-name>
        <url-pattern>/viewFaculty</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>viewAdminServlet</servlet-name>
        <servlet-class>com.endicott.edu.ui.ViewAdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>viewAdminServlet</servlet-name>
        <url-pattern>/viewAdmin</url-pattern>
    </servlet-mapping>
</web-app>